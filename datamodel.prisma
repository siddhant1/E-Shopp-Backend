type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  refreshToken: String
  refreshTokenExpiry: Float
  permissions: [Permission]
  
}
type Item {
  id: ID! @unique
  title: String!
  description: String!
  image: String
  largeImage: String
  price: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  user: User!
}

enum Permission {
  ADMIN
  USER
  ITEMCREATE
  ITEMUPDATE
  PERMISSIONUPDATE
}
